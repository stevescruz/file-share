{
  "compilerOptions": {
    "module": "ES2020",
    "moduleResolution": "node16",
    "esModuleInterop": true,
    "target": "ES2020",
    "sourceMap": true,
    "outDir": "dist",
    "alwaysStrict": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "removeComments": true,
  },
  "include": ["src/**/*"],
  "exclude": [""],
}
